#Create a Jenkinsfile to github account visit
pipeline{
    agent any
    stages{
        stage('Hi'){
            steps{
                echo 'Hello World'
            }
        }
        stage('Aps'){
            steps{
                echo 'Welcome to my Github Repository - ApsarShaik888'
            }
        }
        stage('Fork'){
            steps{
                echo 'Thank you for visiting my github account , feel free to fork the project'
            }
        }
    }
}
